{
	"name": "Flashback",
	"version": "1.0",
	"manifest_version": 2,
	"description": "A screen recording app",
	"key": "",

	"background": {
		"scripts": ["background.js"]
	},
	"browser_action": {
		"default_popup": "popup.htm",
		"default_icon": {
			"16": "images/logo.png",
			"22": "images/logo.png",
			"32": "images/logo.png",
			"48": "images/logo.png",
			"128": "images/logo.png"
		}
	},

	"icons": {
		"16": "images/logo.png",
		"22": "images/logo.png",
		"32": "images/logo.png",
		"48": "images/logo.png",
		"128": "images/logo.png"
	},
	"permissions": ["desktopCapture"]
}
